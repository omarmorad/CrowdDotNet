meta {
  name: Reject Campaign
  type: http
  seq: 3
}

put {
  url: {{baseUrl}}/api/v1/admin/campaigns/{{campaignId}}/reject
  body: json
  auth: inherit
}

headers {
  Content-Type: application/json
  Authorization: Bearer {{adminToken}}
}

body:json {
  {
    "reason": "Insufficient project details",
    "comments": "The campaign lacks detailed information about the project timeline and deliverables. Please provide more comprehensive project documentation."
  }
}

settings {
  encodeUrl: true
}
