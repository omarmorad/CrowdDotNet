meta {
  name: Refresh Token
  type: http
  seq: 3
}

post {
  url: {{baseUrl}}/api/v1/auth/refresh
  body: json
  auth: inherit
}

headers {
  Content-Type: application/json
}

body:json {
  {
    "token": "{{authToken}}",
    "refreshToken": "{{refreshToken}}"
  }
}

settings {
  encodeUrl: true
}
